{
  "kind": "singleType",
  "collectionName": "home_pages",
  "info": {
    "singularName": "home-page",
    "pluralName": "home-pages",
    "displayName": "home page",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "hero_section": {
      "type": "component",
      "repeatable": false,
      "component": "hero.hero-section",
      "required": true
    },
    "slug": {
      "type": "uid"
    },
    "subscribe": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::subscribe.subscribe"
    },
    "counter_section": {
      "type": "component",
      "repeatable": true,
      "component": "text-box.counter"
    },
    "services_section": {
      "type": "component",
      "repeatable": true,
      "component": "text-box.detailed-text-box"
    },
    "review_section_title": {
      "type": "string",
      "required": true,
      "maxLength": 90
    },
    "reviews": {
      "type": "component",
      "repeatable": true,
      "component": "text-box.reviews",
      "required": true
    },
    "video": {
      "allowedTypes": [
        "videos"
      ],
      "type": "media",
      "multiple": false
    }
  }
}
